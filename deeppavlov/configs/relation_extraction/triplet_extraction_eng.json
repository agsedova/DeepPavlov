{
  "chainer": {
    "in": ["x"],
    "pipe": [
      {
        "class_name": "ner_chunker",
        "batch_size": 16,
        "in": ["x"],
        "out": ["text_batch_list", "nums_batch_list", "sentences_offsets_batch_list", "sentences_batch_list"]
      },
      {
        "class_name": "entity_detection_parser",
        "thres_proba": 0.05,
        "o_tag": "O",
        "tags_file": "{NER_PATH}/tag.dict",
        "return_entities_with_tags": true,
        "id": "edp"
      },
      {
        "class_name": "kbqa_entity_linker",
        "id": "linker_entities",
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng",
        "inverted_index_filename": "inverted_index_eng.pickle",
        "entities_list_filename": "entities_list.pickle",
        "q2name_filename": "wiki_eng_q_to_name.pickle",
        "build_inverted_index": false,
        "lemmatize": false,
        "use_descriptions": false,
        "include_mention": false,
        "use_prefix_tree": false,
        "lang": "en"
      },
      {
        "class_name": "ner_chunk_model",
        "ner": {"config_path": "{CONFIGS_PATH}/ner/ner_ontonotes_bert.json"},
        "ner_parser": "#edp",
        "in": ["text_batch_list", "nums_batch_list", "sentences_offsets_batch_list", "sentences_batch_list"],
        "out": ["entity_substr", "entity_offsets", "tags", "probas", "sentences_offsets", "sentences", "tokens", "entity_positions", "sentences_tokens"]
      },
      {
        "class_name": "triplet_extractor",
        "re_model": {"config_path": "{CONFIGS_PATH}/relation_extraction/re_docred.json"},
        "el_model": "#linker_entities",
        "ner_tags": ["ORG", "TIME", "MISC", "LOC", "PER", "NUM"],
        "in": ["entity_substr", "entity_offsets", "tags", "probas", "sentences_offsets", "sentences", "tokens", "entity_positions", "sentences_tokens"],
        "out": ["triplets"],
        "rel_types_filename": "{DOWNLOADS_PATH}/wikidata/rel_types.pickle",
        "wiki_filename": "{DOWNLOADS_PATH}/wikidata/wikidata_lite.hdt"
      }
    ],
    "out": ["triplets"]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "/archive/evseev/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs",
      "NER_PATH": "{MODELS_PATH}/ner_ontonotes_bert"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/tf.txt",
      "{DEEPPAVLOV_PATH}/requirements/bert_dp.txt"
    ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/kbqa/wikidata/wiki_eng_files.tar.gz",
        "subdir": "{DOWNLOADS_PATH}/wikidata_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/kbqa/wikidata/wikidata_lite.hdt",
        "subdir": "{DOWNLOADS_PATH}/wikidata"
      },
      {
        "url": "http://files.deeppavlov.ai/kbqa/wikidata/wikidata_lite.hdt.index.v1-1",
        "subdir": "{DOWNLOADS_PATH}/wikidata"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/relation_extraction/rel_types.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata"
      }
    ]
  }
}